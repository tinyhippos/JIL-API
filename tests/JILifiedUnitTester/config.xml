<?xml version="1.0" encoding="UTF-8"?>
<widget  xmlns="http://www.w3.org/ns/widgets"
          xmlns:JIL="http://www.jil.org/ns/widgets1.2"
         xmlns:its="http://www.w3.org/2005/11/its"
         id="http://example.org/JILifiedUnitTester"
         version="1.0.Beta"
         height="684" width="480"
         viewmodes="floating">
    <JIL:maximum_display_mode height="684" width="480"/>
    <JIL:update href="http://www.jil.org/widgets/" period="2"/>
    <JIL:access network="true" localfs="true" remote_scripts="false"/>
    <JIL:billing required="false"/>
    <icon src="Icon.png"/>
    <content src="index.html" encoding="UTF-8" type="text/html"/>
    <preference name="apikey" value="ea31ad3a23fd2f" readonly="true"/>
    <name short="testwidget">JILified Unit Tester</name>
    <description>A widget for running JIL API tests and compliance level (gold, silver, bronze) verifications.</description>
